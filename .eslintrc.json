{
  "extends": ["next/core-web-vitals", "next/typescript"],
  
    "rules": {
      "no-unused-vars": "off",   // Kullanılmayan değişkenler için uyarıyı devre dışı bırakır
      "no-explicit-any": "off",  // `any` tipi kullanımına uyarı vermez
      "prefer-const": "off"      // `let` yerine `const` kullanımını zorunlu tutmaz
    }
  
  
  

}
